{
  "homepage": "https://ddmihai.github.io/jobfilters",
  "name": "jobfilter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "copy-assets": "xcopy /S /Y \"./src/assets\\*.*\" \"./dist/assets\\\"",

    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "build": "vite build && npm run copy-assets",
    "preview": "vite preview"
  },
  "dependencies": {
    "gh-pages": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-static-copy": "^1.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "sass": "^1.71.1",
    "vite": "^5.1.4"
  }
}
